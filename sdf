#PF-Exer-26

def factorial(number):
    #remove pass and write your logic to find and return the factorial of given number
    fact=1
    if (number==0):
        fact=1
    while(number!=0):
        
        fact*=number
        number-=1
    return fact

def find_strong_numbers(num_list):
     #remove pass and write your logic to find and return the list of strong numbers from the given list
    re_list=[]*len(num_list)
    res_list=[0]*len(num_list)
    for i in range(len(num_list)):
        temp=num_list[i]
        res_list[i]=0
        while(temp!=0):
            rem=temp%10
            temp=temp//10
            res_list[i]+=factorial(rem)
    for i in range(len(num_list)):
        if(num_list[i]==res_list[i] or num_list[i]==2):
            re_list.append(num_list[i])
            return re_list
        else:
            return(-1)
    print(re_list)
num_list=[145, 375, 100, 2, 10, 40585, 0]
strong_num_list=find_strong_numbers(num_list)
print(strong_num_list)
